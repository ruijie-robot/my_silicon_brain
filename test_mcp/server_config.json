{
    "mcpServers": {
        "web_search": {
            "command": "python",
            "args": ["src/mcp_web_search.py"]
        },
        
        "research": {
            "command": "python",
            "args": ["test_mcp/research_server.py"]
        },
        
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "."
            ]
        },
        
        "fetch": {
            "command": "uvx",
            "args": ["mcp-server-fetch"]
        }
    }
}